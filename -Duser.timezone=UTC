-Duser.timezone=UTC
-Dfile.encoding=UTF-8
-Djava.io.tmpdir=%(ENV_JAVA_IO_TMP_DIR)s
-Djava.util.logging.manager=org.apache.logging.log4j.jul.LogManager

-Ddruid.zk.paths.base=/druid
-Ddruid.zk.service.host=%(ENV_ZK_HOSTS)s

-Ddruid.startup.logging.logProperties=true
-Ddruid.request.logging.type=noop
-Ddruid.request.logging.dir=%(ENV_DRUID_LOG_DIR)s


-Ddruid.metadata.storage.type=%(ENV_DRUID_METADATA_STORAGE_TYPE)s
-Ddruid.metadata.storage.connector.connectURI=jdbc:%(ENV_DRUID_METADATA_STORAGE_TYPE)s://%(ENV_DRUID_METADATA_STORAGE_CONN_STR)s
-Ddruid.metadata.storage.connector.user=%(ENV_DRUID_METADATA_STORAGE_USER)s
-Ddruid.metadata.storage.connector.password=%(ENV_DRUID_METADATA_STORAGE_PASS)s


-Ddruid.storage.type=s3
-Ddruid.s3.accessKey=%(ENV_DRUID_AWS_ACCESS_KEY)s
-Ddruid.s3.secretKey=%(ENV_DRUID_AWS_SECRET_KEY)s
-Ddruid.storage.bucket=%(ENV_DRUID_STORAGE_BUCKET)s
-Ddruid.storage.baseKey=%(ENV_DRUID_STORAGE_BASE_KEY)s
-Ddruid.storage.disableAcl=false
-Ddruid.storage.archiveBucket=%(ENV_DRUID_STORAGE_ARCHIVE_BUCKET)s
-Ddruid.storage.archiveBaseKey=%(ENV_DRUID_STORAGE_ARCHIVE_BASE_KEY)s

